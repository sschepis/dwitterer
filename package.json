{
  "name": "dwitterer",
  "publisher": "sschepis",
  "displayName": "Dwitterer",
  "description": "Like dwitter but with more d and in VS Code",
  "icon": "dwitterer-logo.png",
  "version": "0.0.5",
  "repository": {
    "type": "git",
    "url": "https://github.com/sschepis/dwitterer"
  },
  "bugs": {
    "url": "https://github.com/sschepis/dwitterer/issues"
  },
  "engines": {
    "vscode": "^1.58.0"
  },
  "categories": [
    "Snippets",
    "Education"
  ],
  "activationEvents": [
    "onCommand:dwitterer.start",
    "onCommand:dwitterer.doRefactor",
    "onView:dwitter.canvasView"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "views": {
      "explorer": [
        {
          "type": "webview",
          "id": "dwitterer.canvasView",
          "name": "Dwitterer"
        }
      ]
    },
    "commands": [
      {
        "command": "dwitterer.start",
        "title": "Dwitterer"
      },
      {
        "command": "dwitterer.doRefactor",
        "title": "Dwitterer Update"
      }
    ],
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/canvas-snippet.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "format": "prettier \"src/**/*.{js,ts,html}\" --write"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.58.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "prettier": "^2.3.2",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  }
}
